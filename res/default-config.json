{
  "layout": {
    "windowHeight": "24l",
    "windowWidth": "80l",
    "popupHeight": "80%",
    "popupWidth": "80%",
    "items": [
      {
        "identifier": "IAmRoot",
        "size": "100%",
        "childs": [
          "TopArea",
          "MidArea",
          "BotttomArea"
        ],
        "split": "vertical"
      },
      {
        "identifier": "TopArea",
        "size": "3a",
        "childs": ["searchbar"],
        "split": "horizontal"
      },
      {
        "identifier": "MidArea",
        "size": "70%",
        "childs": ["shortcuts", "Central"],
        "split": "horizontal"
      },
      {
        "identifier": "Central",
        "size": "0f",
        "childs": ["panetab", "result_pane"],
        "split": "vertical"
      },
      {
        "identifier": "BotttomArea",
        "size": "3l",
        "childs": ["gauge"],
        "split": "vertical"
      },

      {
        "identifier": "searchbar",
        "size": "0f",
        "childs": [],
        "split": "vertical"
      },
      {
        "identifier": "shortcuts",
        "size": "30%",
        "childs": [],
        "split": "vertical"
      },
      {
        "identifier": "panetab",
        "size": "3a",
        "childs": [],
        "split": "vertical"
      },
      {
        "identifier": "result_pane",
        "size": "0f",
        "childs": [],
        "split": "vertical"
      },
      {
        "identifier": "gauge",
        "size": "3a",
        "childs": [],
        "split": "vertical"
      }
    ]
  },
  "theme": {
    "base_color": "#000000",
    "highlight_color": "#000000",
    "active_color": "#000000",
    "inactive_color": "#000000"
  },
  "keyboard": {
    "<up>": "MoveUp",
    "<down>": "MoveDown",
    "<tab>": "GotoNextWindow",
    "<backtab>": "GotoPrviousWindow",
    "n": "NextTrack",
    "p": "PreviousTrack",
    "<right>": "SeekForward",
    "<left>": "SeekBackward",
    "+": "VolumeUp",
    "-": "VolumeDown",
    "<space>": "PausePlay",
    "s": "ShuffleToggle",
    "r": "RepeatSwitch",
    "q": "Quit",
    "<ctrl>c": "ForceQuit"
  }
}

